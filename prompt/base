      if (adpushup.config.platform === "MOBILE") {
        adpushup.triggerAd("9f8aa9b5-2c42-4296-896e-70772ed1db59");
      } else if ((window.outerWidth <= 768) || (window.outerWidth == 0)) {
        adpushup.triggerAd("9f8aa9b5-2c42-4296-896e-70772ed1db59");
      }
    });
